select from end add at 0

class Solution {
    public void rotate(int[] nums, int k) {
        ArrayList<Integer> num = new ArrayList<>();
        for(int i=0;i<nums.length;i++){
            num.add(nums[i]);
        }
        int l=nums.length-1;
        while(k-->0){
          //  int ele=num.get(l);
            int ele=num.remove(l);
            num.add(0,ele);
        }
       for(int i=0;i<nums.length;i++){
            nums[i]=num.get(i);
        }
 
    }
}
